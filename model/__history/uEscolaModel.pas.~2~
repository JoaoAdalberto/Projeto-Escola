unit uEscolaModel;

interface

uses
  SysUtils;

type
  TEscola = class

  private
    FESCCOD: Integer;
    FESCNOM: String;
    FESCDES: String;
    FESCDATCAD: String;
    FESCCEP: String;
    FESCRUA: String;
    FESCNUM: String;
    FESCCOM: String;
    FESCBAIRRO: String;
    FESCCIDADE: String;
    FESCEST: String;
  published
  //F is for field
  public
    property ESCCOD: Integer read FESCCOD write FESCCOD;
    property ESCNOME: String  read FESCNOM write FESCNOM;
    property ESCDES: string read FESCDES write FESCDES;
    property ESCDATCAD: String read FESCDATCAD write FESCDATCAD;
    property ESCCEP: String read FESCCEP write FESCCEP;
    property ESCRUA: Integer read FESCRUA write FESCRUA;
    property ESCNUM: String  read FESCNUM write FESCNUM;
    property ESCCOM: string read FESCCOM write FESCCOM;
    property ESCBAIRRO: String read FESCBAIRRO write FESCBAIRRO;
    property ESCEST: String read FESCEST write FESCEST;
    property ESCCIDADE: String read FESCCIDADE write FESCCIDADE;



  end;


implementation

end.
